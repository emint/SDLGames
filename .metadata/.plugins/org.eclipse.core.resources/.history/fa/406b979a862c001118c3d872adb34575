#ifndef RECT_H_
#define RECT_H_
#include <string>

class Rect {
  public:
    Rect();
    Rect(int x, int y, size_t width, size_t height);
    virtual ~Rect();


  private:
    int x;
    int y;
    size_t height;
    size_t width;
};

#endif /* RECT_H_ */
